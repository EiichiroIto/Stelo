tests-control ops
testDoRepeatBegin
	| w b t arr |
	w := self newWorld.
	w create: 3 turtles: 1.
	(w breedOf: 3) headingAt: 1 put: 0.
	b := SBytecodeGenerator new.
	b appendValue: false.
	arr := b doRepeatBegin: 5.
	b forward: 1.
	b doRepeatEndLabels: arr.
	self assert: (w var: STurtleVarId xVarNo breed: 3 at: 1) equals: 0.
	t := SThread sworld: w.
	t forBreedNo: 3 index: 1.
	t codes: b code.
	t execute: 1.
	self assert: (w var: STurtleVarId xVarNo breed: 3 at: 1) equals: 5